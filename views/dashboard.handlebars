<!-- Subpage Banner -->
<section class="subpage">
	<div class="container">
		<div class="row">
			<div class="col-span-12 text-center">
				<h1 style="subpage-heading">Dashboard</h1>
			</div>
		</div>
	</div>
</section>

<div class="container sub-container-divider">
	<div class="row">
		<div class="col-12">
			<h2>Welcome To CryptoCurrency Tracker!</h2>
			<a href="/logout">
				<button id='logout' class="float-left submit-button">Logout</button>
			</a>
		</div>
		<div class='col-12'>
			<table class="table">
				<thead>
					<tr>
						<th scope="col">First Name</th>
						<th scope="col">Last Name</th>
						<th scope="col">Username</th>
						<th scope="col">fav1</th>
						<th scope="col">fav2</th>
						<th scope="col">fav3</th>
					</tr>
				</thead>
				<tbody>
					{{#each userData}}
					<tr>
						<td>{{this.firstname}}</td>
						<td>{{this.lastname}}</td>
						<td>{{this.username}}</td>
						<td>{{this.fav1}}</td>
						<td>{{this.fav2}}</td>
						<td>{{this.fav3}}</td>
					</tr>
					{{/each}}
				</tbody>
			</table>
		</div>
	</div>
</div>

<div class="row">
	<div class="col-md-12">
		<div class="panel panel-primary">
			<div class="panel-heading">
				<h2 class="panel-title">Tracker</h2>
			</div>
			<div class="panel-body">
				<table class="table table-hover" id="coin-tracker">
					<thead>
						<tr>
							<th>Name</th>
							<th>Symbol</th>
							<th>rank</th>
							<th>Price</th>
							<th>Market Cap</th>
							<th>Percent Change(24h)</th>
						</tr>
					</thead>
					<tbody>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
